    private void btnGeraOrdemProdActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnGeraOrdemProdActionPerformed

    try{        
        for (int i = 0; i < listaItensOrdem.size(); i++){
            listaItensOrdem.get(i).setSituacao('3');
            new OADItem().salvar(listaItensOrdem.get(i));
            Pedido pedido = new Pedido();
            pedido.setCodigo(listaItensOrdem.get(i).getPedido().getCodigo());
            pedido = new OADPedido().carrega(pedido);
            if (pedido.getSituacao().equals('1')){
                pedido.setSituacao('2');
                new OADPedido().salvar(pedido);
            }
            
        }
        pedidosComProduto = new OADItem().ListaPedidosComProduto(produtoSelecionado);
        for (int i = 0; i < pedidosComProduto.size(); i++){
            listaItensEmCadastramentoPedido = new OADItem().ListaPeloPedido(pedidosComProduto.get(i));
            if ((listaItensEmCadastramentoPedido == null) || (listaItensEmCadastramentoPedido.isEmpty())){
                fecharPedido(pedidosComProduto.get(i));
            }
        }
        
        novaOrdemProducao = new OrdemProducao();
        novaOrdemProducao.setDataInclusao( new Date(System.currentTimeMillis()));
        novaOrdemProducao.setSituacao('1');
        novaOrdemProducao.setItens(listaItensOrdem);

        if(new OADOrdemProducao().salvar(novaOrdemProducao)){
            JOptionPane.showMessageDialog(this, "Ordem de Produção Gerada com sucesso!", "", JOptionPane.INFORMATION_MESSAGE);        
            gerarRelatorioOrdemProducao();
            pesquisaProdutoPedidos();
        }else{
            JOptionPane.showMessageDialog(this, "Erro ao gerar Ordem de Produção!", "", JOptionPane.ERROR_MESSAGE);        
        }
    }catch(Exception e){
        System.out.println(e.toString());
    }
     
    }//GEN-LAST:event_btnGeraOrdemProdActionPerformed
   
    private void fecharPedido(Pedido ped) {
        ped.setDataFechamento(new Date(System.currentTimeMillis()));
        ped.setSituacao('3');
        new OADPedido().salvar(ped);        
    }


