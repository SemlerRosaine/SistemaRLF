    public void consultarProduto(Produto pcons) {
        tmConsultaProdutoPedidos.setRowCount(0);
        produtoSelecionado = new OADProduto().CarregaPelaReferencia(pcons);
             
        if ((produtoSelecionado == null) || ((produtoSelecionado.getCodigo() == null || produtoSelecionado.getCodigo().equals(0)) && produtoSelecionado.getDescricao().trim().isEmpty())){
            JOptionPane.showMessageDialog(this, "Produto não encontrado!", "", JOptionPane.INFORMATION_MESSAGE);            
            produtoSelecionado = null;
            txtDescProdPedido.setText("");
            txtCodProdPedido.requestFocus();
            btnGeraOrdemProd.setEnabled(false);
        }else{
            txtDescProdPedido.setText(produtoSelecionado.getDescricao());
            listaItensOrdem = new OADItem().ListaPeloProduto(produtoSelecionado);
            for (int i = 0; i < listaItensOrdem.size(); i++){
                tmConsultaProdutoPedidos.addRow(listaItensOrdem.get(i).getLinhaOrdem());
            }
            btnGeraOrdemProd.setEnabled(true);
        }                        
    }

    private void gerarRelatorioOrdemProducao() {

        new File("Relatorios").mkdir();
        // criação do documento
          Document document = new Document();
          String nomePdf = "Relatorios/OrdemProd_"+ String.valueOf(produtoSelecionado.getCodigoReferencia()) + String.valueOf(new SimpleDateFormat("_yyyy_MM_dd_HHmmss").format(new java.util.Date())) + ".pdf";
          
          try {
              PdfWriter.getInstance(document, new FileOutputStream(nomePdf));
              document.open();
             
              // adicionando um parágrafo no documento
              document.add(new Paragraph("Rosy Lãs e Fios - Ordem de Produção"));
              document.add(new Paragraph("____________________________________________"));
              document.add(new Paragraph("Produto: " + String.valueOf(produtoSelecionado.getCodigoReferencia()) + " - " + String.valueOf(produtoSelecionado.getDescricao())));
              document.add(new Paragraph("         "));
              document.add(geraTabelaGrade());
              
          }catch(DocumentException de) {
              System.err.println(de.getMessage());
          }
          catch(IOException ioe) {
              System.err.println(ioe.getMessage());
          }
          document.close();
        try {
            File arq = new File(nomePdf);
            Desktop.getDesktop().open(arq);  
        } catch (IOException ex) {
            Logger.getLogger(FormGeraOrdemProducao.class.getName()).log(Level.SEVERE, null, ex);
        }
    }

    private Element geraTabelaGrade() throws DocumentException {
        PdfPTable table = new PdfPTable(5);
        table.setTotalWidth(515);
        table.setLockedWidth(true);
        table.setWidths(new float[]{1, 3, 1, 1, 1});
        
        PdfPCell cell;
        cell = new PdfPCell(new Phrase("Pedido"));
        cell.setHorizontalAlignment(Element.ALIGN_CENTER);
        table.addCell(cell);
        cell = new PdfPCell(new Phrase("Cliente"));
        cell.setHorizontalAlignment(Element.ALIGN_CENTER);
        table.addCell(cell);
        cell = new PdfPCell(new Phrase("Quantidade"));
        cell.setHorizontalAlignment(Element.ALIGN_CENTER);
        table.addCell(cell);
        cell = new PdfPCell(new Phrase("Tamanho"));
        cell.setHorizontalAlignment(Element.ALIGN_CENTER);
        table.addCell(cell);
        cell = new PdfPCell(new Phrase("Cor"));
        cell.setHorizontalAlignment(Element.ALIGN_CENTER);
        table.addCell(cell);
                
        // we add the four remaining cells with addCell()
        String quebra_ant = "";
        String quebra_atu = "";
        Integer qtdeQuebra = 0;
        Integer qtdeTotal = 0;
        for (int i = 0; i < tmConsultaProdutoPedidos.getRowCount(); i++){
            quebra_ant = quebra_atu;
            quebra_atu = tmConsultaProdutoPedidos.getValueAt(i, 3).toString();
            if (i > 0 && !quebra_ant.equals(quebra_atu)){
                cell = new PdfPCell(new Phrase(" "));
                cell.setColspan(2);
                cell.setHorizontalAlignment(Element.ALIGN_RIGHT);
                table.addCell(cell);
                
                cell = new PdfPCell(new Phrase(qtdeQuebra.toString()));
                cell.setHorizontalAlignment(Element.ALIGN_CENTER);
                table.addCell(cell);
                
                cell = new PdfPCell(new Phrase(" "));
                cell.setColspan(2);
                table.addCell(cell);
                qtdeTotal = qtdeTotal + qtdeQuebra;                
                qtdeQuebra = 0;

                cell = new PdfPCell(new Phrase(" "));
                cell.setColspan(5);
                table.addCell(cell);                
            }
            table.addCell(tmConsultaProdutoPedidos.getValueAt(i, 0).toString());
            table.addCell(tmConsultaProdutoPedidos.getValueAt(i, 1).toString());
            cell = new PdfPCell(new Phrase(tmConsultaProdutoPedidos.getValueAt(i, 2).toString()));
            cell.setHorizontalAlignment(Element.ALIGN_CENTER);
            table.addCell(cell);
            qtdeQuebra = qtdeQuebra + Integer.valueOf(tmConsultaProdutoPedidos.getValueAt(i, 2).toString());

            cell = new PdfPCell(new Phrase(tmConsultaProdutoPedidos.getValueAt(i, 3).toString()));
            cell.setHorizontalAlignment(Element.ALIGN_CENTER);
            table.addCell(cell);
            table.addCell(tmConsultaProdutoPedidos.getValueAt(i, 4).toString());
        }        
        cell = new PdfPCell(new Phrase(" "));
        cell.setColspan(2);
        cell.setHorizontalAlignment(Element.ALIGN_RIGHT);
        table.addCell(cell);

        cell = new PdfPCell(new Phrase(qtdeQuebra.toString()));
        cell.setHorizontalAlignment(Element.ALIGN_CENTER);
        table.addCell(cell);
        qtdeTotal = qtdeTotal + qtdeQuebra;              

        cell = new PdfPCell(new Phrase(" "));
        cell.setColspan(2);
        table.addCell(cell);

        cell = new PdfPCell(new Phrase(" "));
        cell.setColspan(5);
        table.addCell(cell);
        
        cell = new PdfPCell(new Phrase("TOTAL DE PEÇAS PARA PRODUÇÃO"));
        cell.setColspan(2);
        cell.setHorizontalAlignment(Element.ALIGN_RIGHT);
        table.addCell(cell);

        cell = new PdfPCell(new Phrase(qtdeTotal.toString()));
        cell.setHorizontalAlignment(Element.ALIGN_CENTER);
        table.addCell(cell);

        cell = new PdfPCell(new Phrase(" "));
        cell.setColspan(2);
        table.addCell(cell);
        
        return table;
    }




