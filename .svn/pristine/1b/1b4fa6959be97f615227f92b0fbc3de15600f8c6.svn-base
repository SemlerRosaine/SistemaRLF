    private void btnAlterarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnAlterarActionPerformed
        // TODO add your handling code here:
        Pedido ped;
        if(gridConsulta.getSelectedRowCount()==0){
            JOptionPane.showInputDialog("Selecione um pedido para efetivar  a operação");
        }else{
            ped = pedidos.get(gridConsulta.getSelectedRow());
            Principal.TELA_PRINCIPAL.abrirJanelaPedido(null, ped);
        }
        
               
 
    }//GEN-LAST:event_btnAlterarActionPerformed

    private void btnExcluirActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnExcluirActionPerformed
        // TODO add your handling code here:
        Pedido p = new Pedido();
        p.setCodigo(Integer.valueOf(tmConsultaPedido.getValueAt(gridConsulta.getSelectedRow(), 0).toString()));
        p = new OADPedido().carrega(p);
        consultarPedido(null);
        if(gridConsulta.getSelectedRowCount()==0){
            JOptionPane.showInputDialog("Selecione um pedido para efetivar  a operação");
        }else{
            p = pedidos.get(gridConsulta.getSelectedRow());
            if (JOptionPane.showConfirmDialog(this, "Confirma a exclusão do cliente?", "Confirmação", JOptionPane.YES_NO_OPTION) == JOptionPane.YES_OPTION) {
            new OADPedido().remove(p);
        }
        }
       
        
        p = null;
       
       
    }//GEN-LAST:event_btnExcluirActionPerformed

    private void btnPesquisarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnPesquisarActionPerformed
        Pedido pcons = new Pedido();
        if (!txtCodPedido.getText().trim().isEmpty()){
            pcons.setCodigo(Integer.valueOf(txtCodPedido.getText()));
        }else if ((!txtDataPedido.getText().trim().isEmpty()))
                pcons.setDataInclusao(Date.valueOf(txtDataPedido.getText()));
             else
                pcons = null;
        consultarPedido(pcons);
    }//GEN-LAST:event_btnPesquisarActionPerformed

